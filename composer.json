{
    "name": "course-hero/symfony-cron-bundle",
    "description": "Cron-related utilities for Symfony2",
    "keywords": ["cron", "symfony"],
    "type": "symfony-bundle",
    "license": "Apache-2",
    "authors": [
        {
            "name": "Chris Verges",
            "email": "cverges@coursehero.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">= 5.3.3",
        "symfony/symfony": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3"
    },
    "autoload": {
        "psr-0": {
            "SymfonyCron": "src/",
            "SymfonyCron\\Tests": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/course-hero/symfony-cron-bundle",
            "options": {
                "ssl": {
                    "verify_peer": "true"
                }
            }
        }
    ]
}
